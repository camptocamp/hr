<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record model="ir.module.category" id="module_category_hubspot">
        <field name="name">Hubspot</field>

    </record>

    <record id="group_hubspot_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_category_hubspot"/>
    </record>

    <record id="group_hubspot_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="module_category_hubspot"/>
        <field name="implied_ids"
               eval="[(4, ref('hubspot.group_hubspot_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>

    </record>
</odoo>

