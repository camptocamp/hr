<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <act_window
            id="action_backbone_link"
            name="Links"
            res_model="backbone.link"
            view_mode="tree,form,graph,pivot"/>

    <act_window
            id="action_backbone_device"
            name="Devices"
            res_model="backbone.device"
            view_mode="tree,form,graph,pivot"/>

    <act_window
            id="action_backbone_pop"
            name="POPs"
            res_model="backbone.pop"
            view_mode="tree,form,graph,pivot"/>

    <act_window
            id="action_backbone_xco"
            name="XConnects"
            res_model="backbone.xco"
            view_mode="tree,form,graph,pivot"/>

    <act_window
            id="action_cympa_link"
            name="CYMPA Links"
            res_model="backbone.cympa.link"
            view_mode="tree,form,graph,pivot"/>

    <record model="ir.actions.server" id="action_settings">
        <field name="name">backbone settings</field>
        <field name="model_id" ref="model_backbone_settings"/>
        <field name="state">code</field>
        <field name="code">
            action = model.action_settings()
        </field>
    </record>

</odoo>