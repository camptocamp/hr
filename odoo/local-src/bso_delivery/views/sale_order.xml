<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sale_order_create_project_del_button" model="ir.ui.view">
        <field name="name">sale.order.create.project.button</field>
        <field name="model">sale.order</field>
        <field name="inherit_id"
               ref="bso_dealsheet.sale_order_dealsheet"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='dealsheet_id']" position="after">
                <field name="delivery_project_id" widget="many2onebutton" />
            </xpath>
        </field>
    </record>
</odoo>
