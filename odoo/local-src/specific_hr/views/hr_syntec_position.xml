<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_syntec_position_form" model="ir.ui.view">
        <field name="name">hr.syntec.position.form</field>
        <field name="model">hr.syntec.position</field>
        <field name="arch" type="xml">
            <form string="Syntec Positions">
                <group>
                    <field name="name"/>
                </group>
            </form>
        </field>
    </record>
    <record id="view_syntec_position_list" model="ir.ui.view">
        <field name="name">hr.syntec.position.list</field>
        <field name="model">hr.syntec.position</field>
        <field eval="8" name="priority"/>
        <field name="arch" type="xml">
            <tree string="Syntec Positions">
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="open_view_syntec_position_form" model="ir.actions.act_window">
        <field name="name">Syntec Positions</field>
        <field name="res_model">hr.syntec.position</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem action="open_view_syntec_position_form" id="menu_view_syntec_position_form"
        parent="hr.menu_human_resources_configuration" sequence="1" />

</odoo>